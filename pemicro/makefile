HELP:
 @echo "*********************************************************
 @echo "* Targets: ALL
 @echo "*        : QE128.S8P
 @echo "*        : PA32.S8P
 @echo "*********************************************************
 @echo "* Defines: Nothing yet
 @echo "*********************************************************

O = -r42 -rel- -h- -fq+ -e+ -l- -ls- -m- -s+ $(O)

LIST = qe128.s8p \
       pa32.s8p

all: $(LIST)

.asm.s8p:
  @asm8 $* $(O) -b9s08$*.s8p
